# bash
~/.bashrc ~/setup/bash/.bashrc

# zsh
~/.zshrc ~/setup/zsh/.zshrc

# vim
~/.vimrc ~/setup/vim/.vimrc

# i3
~/.config/i3/config ~/setup/i3/config
/etc/i3status.conf ~/setup/i3/i3status.conf

# dunst
~/.config/dunst/dunstrc ~/setup/dunst/dunstrc

# poweline-shell
~/.config/powerline-shell/config.json ~/setup/powerline-shell/config.json

# polybar
~/.config/polybar/config.ini ~/setup/polybar/config.ini
~/.config/polybar/modules.ini ~/setup/polybar/modules.ini
~/.config/polybar/launch.sh ~/setup/polybar/launch.sh

# tmux
~/.tmux.conf ~/setup/tmux/.tmux.conf

# scripts
/bin/start-haskell ~/setup/scripts/start-haskell
/bin/attach-monitor ~/setup/scripts/attach-monitor
/bin/detach-monitor ~/setup/scripts/detach-monitor
